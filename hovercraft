#include<stdio.h>
int main()
{
	int hovercraft;
	int total = 10;
	int cost;
	int totalcost;
	int selling ;
	int insuarance;
	
	 printf(" enter the number of hovercrafts:\n");
	 scanf("%d",&hovercraft);
	
	insuarance= hovercraft* 1,000,000; 
	 cost= hovercraft*2,000,000 ;    // cost of each
	 totalcost = cost + insuarance ;   // insuarance
	  
	selling= hovercraft* 3,000,000;   // selling price of each 
	
	if (selling > totalcost ) {
		printf("profit");
	}

	 else if(selling <= totalcost ){
	 	printf("loss");
	 	
	 }
	
return 0;

}
